{
  "profile": "django",
  "indent": 4,
  "max_line_length": 120,
  "max_attribute_length": 120,
  "format_css": true,
  "format_js": true,
  "exclude": "node_modules,venv,.venv,staticfiles,static/css/tailwind-output.css",
  "extension": "html",
  "ignore": "H006,H030,H031",
  "preserve_leading_space": false,
  "preserve_blank_lines": true,
  "blank_line_after_tag": "load,extends,include",
  "blank_line_before_tag": "block,endblock",
  "custom_blocks": "",
  "custom_html": "",
  "linter_output_format": "{filename}:{line}: {code} {message}",
  "require_pragma": false,
  "line_break_after_multiline_tag": false,
  "close_void_tags": false,
  "no_line_after_yaml": false,
  "no_function_formatting": false,
  "no_set_formatting": false,
  "use_gitignore": true
}
